{
  "compilerOptions": {
    "target": "es5",
    "noImplicitAny": true,
    "sourceMap": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "diagnostics": true,
    "listFiles": true,
    "pretty": true,
    "noEmitOnError": false,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "declaration": true,
    "strict": true,
    "outDir": "./dist"
  },
  "include": [
    "src/**/*.ts"
  ]
}
